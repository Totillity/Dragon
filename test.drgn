
class Person {
    attr name: str;
    attr age: int;

    method say_hi(to: str) -> void {
        print("Hi, ");
        print(to);
        print(", I'm ");
        print(self.name);
        print("\n");
    }
}


class RealPerson(Person) {

}


class Holder {
    attr person: Person;
}


def main() -> int {
    var holder: Holder = new Holder();

    var bob: RealPerson = new RealPerson();
    bob.name = "Bob";
    bob.age = 7;
    bob.say_hi("Jane");

    holder.person = bob;

    holder.person.say_hi("Jane");

    return 0;
}
