

class Test {
    attr num: int;

    new(num: int) {
        self.num = num;
    }
}


class Test2 (Test) {
    attr thing: int;

    new(num: int, thing: int) {
        self.num = num;
        self.thing = thing;
    }
}



def main() -> int {
    var test2: Test2 = new Test2(3, 7);
    var test: Test = test2;

    print(test2.num);
    print(test.num);
    print(test2.thing);
    print((test as Test2).thing);
    return 0;
}
